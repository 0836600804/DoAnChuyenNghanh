@model QuanLyBanSach.Models.AdminSach

@{
    ViewBag.Title = "Detail_AD";
    Layout = "~/Views/Shared/admin.cshtml";
}

<h1 class="h3 mb-2 text-gray-800">Chi tiết Admin</h1>

<div class="card shadow mb-4">
    <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary">Thông tin chi tiết</h6>
    </div>
    <div class="card-body">
        <dl class="row">
            <dt class="col-sm-4">Mã Quản lý</dt>
            <dd class="col-sm-8">@Html.DisplayFor(model => model.MaAD)</dd>
            <dt class="col-sm-4">Tên Quản lý</dt>
            <dd class="col-sm-8">@Html.DisplayFor(model => model.TenAD)</dd>
            <dt class="col-sm-4">Tài khoản</dt>
            <dd class="col-sm-8">@Html.DisplayFor(model => model.TaiKhoan)</dd>
            <dt class="col-sm-4">Mật khẩu</dt>
            <dd class="col-sm-8">@Html.DisplayFor(model => model.MatKhau)</dd>
        </dl>
    </div>
    <div class="card-footer">
        @Html.ActionLink("Quay lại", "List_AD", null, new { @class = "btn btn-secondary" })
    </div>
</div>
